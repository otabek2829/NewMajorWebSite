{{> Dashboard/Navbar/navbar}}
<section>
    <div class="prise">
        <h1 class="title mb-5">{{title}}</h1>
        {{!-- Price List Group Items --}}
        <div class="price-list-group-items">
            <div class="container">
                <div class="row flex-column">
                    <div class="col-12 mb-5 ">
                        <div class="list-group" id="list-tab" role="tablist">

                            <!-- Button father -->

                            <!-- Разработка сайтов -->
                            <a class="list-group-item list-group-item-action " id="list-home-list"
                                data-toggle="list" href="/dashboard/price/web-site" role="tab" aria-controls="home">Веб-разработка</a>

                            <!-- Дизайн -->
                            <a class="list-group-item list-group-item-action" id="list-messages-list" data-toggle="list"
                                href="/dashboard/price/design" role="tab" aria-controls="messages">Дизайн</a>

                            <!-- SMM-->
                            <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list"
                                href="/dashboard/price/smm" role="tab" aria-controls="settings">SMM</a>

                            <!-- Техническая поддержка -->
                            <a class="list-group-item list-group-item-action" id="list-support-list" data-toggle="list"
                                href="/dashboard/price/technical-support" role="tab" aria-controls="support">Техническая поддержка</a>
                        </div>
                    </div>


                    <div class="col-12">
                        <div class="tab-content" id="nav-tabContent">
                               
                            <!-- Button bosilganda u haqida ma\lumot chiqishi -->

                            <!-- Разработка сайтов -->
                            <div class="tab-pane fade show active" id="list-home" role="tabpanel"
                                aria-labelledby="list-home-list">
                               {{#each dataBasePrice}}
                                <div class="body-items-text">
                                    <h6>{{this.title}}</h6>
                                    <h6 class="formatPrice">от <span>{{this.price}}</span> сум</h6>
                                </div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="/javascripts/app.js"></script>